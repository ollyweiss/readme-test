    "/{version}/commerce/orders": {
      "get": {
        "tags": [
          "Orders"
        ],
        "summary": "List Orders",
        "description": "List Orders, sorted by their modifiedOn date in reverse chronological order",
        "operationId": "getOrders",
        "parameters": [
          {
            "name": "version",
            "in": "path",
            "required": true,
            "type": "string"
          },
          {
            "name": "modifiedAfter",
            "in": "query",
            "required": false,
            "type": "string",
            "format": "date-time"
          },
          {
            "name": "modifiedBefore",
            "in": "query",
            "required": false,
            "type": "string",
            "format": "date-time"
          },
          {
            "name": "cursor",
            "in": "query",
            "required": false,
            "type": "string",
            "maxLength": 25
          },
          {
            "name": "fulfillmentStatus",
            "in": "query",
            "required": false,
            "type": "string",
            "enum": [
              "PENDING",
              "FULFILLED",
              "CANCELED"
            ]
          }
        ],
        "responses": {
          "200": {
            "description": "Return a list of Orders",
            "schema": {
              "$ref": "OrderFulfillmentRequest"
            }
          }
        },
        "x-event-name": "OrdersApi_GetOrders"
      }
    },
    "/{version}/commerce/orders/{id}": {
      "get": {
        "tags": [
          "Orders"
        ],
        "summary": "Get one Order",
        "description": "Get one Order",
        "operationId": "getOrder",
        "parameters": [
          {
            "name": "version",
            "in": "path",
            "required": true,
            "type": "string"
          },
          {
            "name": "id",
            "in": "path",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "Return one Order",
            "schema": {
              "$ref": "Order"
            }
          }
        },
        "x-event-name": "OrdersApi_GetOneOrder"
      }
    },
    "/{version}/commerce/orders/{id}/fulfillments": {
      "post": {
        "tags": [
          "Orders"
        ],
        "summary": "Fulfill an Order",
        "operationId": "fulfillOrder",
        "parameters": [
          {
            "name": "version",
            "in": "path",
            "required": true,
            "type": "string"
